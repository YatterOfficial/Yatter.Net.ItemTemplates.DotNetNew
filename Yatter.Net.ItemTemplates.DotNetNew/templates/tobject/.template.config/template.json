{
  "$schema": "http://json.schemastore.org/template",
  "author": "Anthony Harrison",
  "classifications": [ "Common", "Code", "Yatter", "Invigoration", "TObject" ],
  "identity": "Yatter.Invigoration.TObject",
  "name": "Yatter TObject file",
  "shortName": "yatter-tobject",
  "description": "A default C# TObject file using Yatter's Invigoration Pattern",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "InvigorationTObjects.TObject",
  "preferNameDirectory":true,
  "symbols":{
    "FileName": {
      "type": "parameter",
      "isRequired": true,
      "datatype": "string",
      "description": "Renames tobject file name",
      "FileRename": "TObjectClass",
      "defaultValue": "TObjectClass"
      },
    "TOClassName": {
      "type": "parameter",
      "isRequired": true,
      "datatype": "string",
      "description": "Renames TO class name",
      "defaultValue": "TOClassName",
      "replaces":"TOClass"
    }
  }
}


