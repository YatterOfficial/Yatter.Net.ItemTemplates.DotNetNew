{
  "$schema": "http://json.schemastore.org/template",
  "author": "Anthony Harrison",
  "classifications": [ "Web", "Misc" ],
  "identity": "Yatter.Invigoration.TActor",
  "name": "Yatter TActor file",
  "shortName": "yatter-tactor",
  "defaultName": "yatter-tactor",
  "description": "A default C# TActor file using Yatter's Invigoration Pattern",
  "tags": {
    "language": "C#",
    "type": "item"
  },
  "sourceName": "InvigorationTActors.TActor",
  "preferNameDirectory": true,
  "symbols": {
    "Filename": {
      "type": "parameter",
      "isRequired": true,
      "datatype": "string",
      "description": "Renames tactor file name",
      "FileRename": "TActorClass",
      "defaultValue": "TActorClass"
    },
    "TAClassName": {
      "type": "parameter",
      "isRequired": true,
      "datatype": "string",
      "description": "Renames TA class name",
      "defaultValue": "TAClassName",
      "replaces": "TAClass"
    },
    "TOClassName": {
      "type": "parameter",
      "datatype": "string",
      "description": "Renames TO class name",
      "defaultValue": "TOClassName",
      "replaces": "TODefault"
    },
    "TRClassName": {
      "type": "parameter",
      "datatype": "string",
      "description": "Renames TR class name",
      "defaultValue": "TRClassName",
      "replaces": "TRNotImplemented"
    },
    "IsAsync": {
      "type": "parameter",
      "dataType": "bool",
      "description": "Configures TA ActAsync or Act alternatives",
      "defaultValue": "true"
    }
  }
}
